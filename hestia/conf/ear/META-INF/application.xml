<?xml version="1.0" encoding="UTF-8"?>
<application xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:application="http://java.sun.com/xml/ns/javaee/application_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/application_5.xsd"
	version="5">
	<display-name>hestia-war-room</display-name>
	<module>
		<java>@application.name.jpa@.jar</java>
	</module>
	<module>
		<ejb>@application.name.ejb@.jar</ejb>
	</module>
	<module>
		<web>
			<web-uri>@application.name@.war</web-uri>
			<context-root>@context.path@</context-root>
		</web>
	</module>
</application>